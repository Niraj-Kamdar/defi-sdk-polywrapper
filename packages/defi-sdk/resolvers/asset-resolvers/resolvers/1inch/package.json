{
  "name": "@defiwrapper/1inch-asset-resolver",
  "description": "1Inch Asset Resolver",
  "private": true,
  "version": "1.0.1",
  "scripts": {
    "lint": "eslint . --ext .ts --ignore-path ../../../../../../.eslintignore",
    "lint:fix": "eslint . --ext .ts --fix --ignore-path ../../../../../../.eslintignore",
    "format": "prettier --write \"./**/*.{js,jsx,json,ts}\"",
    "build": "npx polywrap build",
    "codegen": "npx polywrap codegen",
    "watch": "npx polywrap build -i -e 1inch.token-resolver.defiwrapper.eth -w",
    "infra:up": "npx polywrap infra up",
    "infra:down": "npx polywrap infra down",
    "deploy": "npx polywrap deploy",
    "test": "yarn infra:up && yarn test:e2e && yarn infra:down",
    "test:e2e": "jest --passWithNoTests --runInBand --verbose",
    "compile:config:workflow": "tsc ./config/config.workflow.ts --esModuleInterop --resolveJsonModule",
    "workflow": "yarn infra:up && yarn compile:config:workflow && sleep 2 && npx polywrap run -c ./config/config.workflow.js && yarn infra:down"
  },
  "dependencies": {
    "@polywrap/wasm-as": "0.7.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "polywrap": "0.7.0",
    "@polywrap/ens-resolver-plugin-js": "0.7.0",
    "@polywrap/ipfs-plugin-js": "0.7.0",
    "@polywrap/ethereum-plugin-js": "0.7.0",
    "@polywrap/test-env-js": "0.7.0",
    "@polywrap/wasm-as": "0.7.0",
    "assemblyscript": "0.19.23",
    "eslint": "^7.32.0",
    "ethers": "5.0.7",
    "jest": "^27.4.2",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.3"
  }
}
