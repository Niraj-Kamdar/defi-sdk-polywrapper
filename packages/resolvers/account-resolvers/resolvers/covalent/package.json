{
  "name": "@defiwrapper/covalent-account-resolver",
  "description": "Covalent account resolver",
  "private": true,
  "version": "1.0.1",
  "scripts": {
    "lint": "eslint . --ext .ts --ignore-path ../../../../../.eslintignore",
    "lint:fix": "eslint . --ext .ts --fix --ignore-path ../../../../../.eslintignore",
    "format": "prettier --write \"./**/*.{js,jsx,json,ts}\"",
    "build": "npx polywrap build",
    "codegen": "npx polywrap codegen",
    "watch": "npx polywrap build -i -e ethereum.protocol-resolver.defiwrapper.eth -w",
    "test:env:up": "npx polywrap infra up --modules=eth-ens-ipfs",
    "test:env:down": "npx polywrap infra down --modules=eth-ens-ipfs",
    "deploy": "npx polywrap deploy",
    "test": "yarn test:e2e",
    "test:e2e": "jest --passWithNoTests --runInBand --verbose"
  },
  "devDependencies": {
    "polywrap": "0.7.0",
    "@polywrap/ens-resolver-plugin-js": "0.7.0",
    "@polywrap/ipfs-plugin-js": "0.7.0",
    "@polywrap/ethereum-plugin-js": "0.7.0",
    "@polywrap/test-env-js": "0.7.0",
    "@polywrap/wasm-as": "0.7.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "as-big": "^0.1.5",
    "assemblyscript": "0.19.1",
    "axios": "^0.27.2",
    "eslint": "^7.32.0",
    "ethers": "5.0.7",
    "jest": "^27.4.2",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  }
}
