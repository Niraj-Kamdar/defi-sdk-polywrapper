{
  "name": "@defiwrapper/coingecko-price-resolver",
  "description": "Coingecko price resolver",
  "private": true,
  "version": "1.0.1",
  "scripts": {
    "lint": "eslint . --ext .ts --ignore-path ../../../../../.eslintignore",
    "lint:fix": "eslint . --ext .ts --fix --ignore-path ../../../../../.eslintignore",
    "format": "prettier --write \"./**/*.{js,jsx,json,ts}\"",
    "build": "npx polywrap build -c ./clientConfig.ts",
    "codegen": "npx polywrap codegen",
    "watch": "npx polywrap build -i -e ethereum.protocol-resolver.defiwrapper.eth -w",
    "deploy": "npx polywrap deploy",
    "infra:up": "npx polywrap infra up",
    "infra:down": "npx polywrap infra down",
    "test": "yarn test:codegen && yarn infra:up && yarn test:e2e && yarn infra:down",
    "test:e2e": "jest --passWithNoTests --runInBand --verbose",
    "test:codegen": "npx polywrap codegen -m ./src/__tests__/types/polywrap.app.yaml -g ./src/__tests__/types/wrap"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "polywrap": "0.7.0",
    "@polywrap/ethereum-plugin-js": "0.7.0",
    "@polywrap/test-env-js": "0.7.0",
    "@polywrap/wasm-as": "0.7.0",
    "assemblyscript": "0.19.23",
    "eslint": "^7.32.0",
    "ethers": "5.0.7",
    "jest": "^27.4.2",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  }
}
