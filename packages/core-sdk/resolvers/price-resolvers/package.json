{
  "name": "@defiwrapper/price-resolvers",
  "description": "Price Resolvers",
  "private": true,
  "version": "0.0.1-prealpha.70",
  "scripts": {
    "prebuild": "lerna run build --scope @defiwrapper/token-resolvers && lerna run build --scope @defiwrapper/coingecko",
    "build": "yarn build:interface && yarn build:resolvers",
    "build:interface": "lerna run build --scope @defiwrapper/price-resolver-interface",
    "build:resolvers": "lerna run build --scope @defiwrapper/*-price-resolver",
    "test": "yarn test:resolvers",
    "test:resolvers": "lerna run test --scope @defiwrapper/*-price-resolver"
  }
}
